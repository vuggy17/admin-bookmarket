<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.admin_bookmarket">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".CoreApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Adminbookmarket">
        <activity android:name=".EditProfileActivity"></activity>
        <activity android:name=".ProfileActivity" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.ForgotPassword" />
        <activity android:name=".StatisticActivity" />
        <activity android:name=".OrderDetail" />
        <activity android:name=".SearchActivity" />
        <activity android:name=".AddItemActivity" />
        <activity android:name=".ItemDetailActivity" />
        <activity android:name=".ui.login.LoginActivity" />
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.Adminbookmarket.NoActionBar" />
    </application>

</manifest>